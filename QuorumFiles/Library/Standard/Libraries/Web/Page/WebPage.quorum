package Libraries.Web.Page

use Libraries.Containers.List
use Libraries.Containers.Blueprints.Iterator
use Libraries.Web.Page.WebGenerator
use Libraries.Web.Page.GlobalAttributeAccepter
use Libraries.Web.Page.WebPageHeader
use Libraries.Web.Page.DocumentType
use Libraries.Web.Page.Body

/*
The WebPage class represents HTML's (Hypertext Markup Language) option tag which 
defines the document as an html document. 
You can find more information about this tag at: 
<a href="http://www.w3schools.com/tags/tag_html.asp">The html attribute</a>.

Attribute: Example
use Libraries.Web.Page.all
class Main
   action main
      WebPage page
      
      Body body
      page:SetBody(body)

      page:AddToBody(body)
      output page:Generate()
   end
end
*/
class WebPage is WebGenerator, GlobalAttributeAccepter
    DocumentType documentType
    WebPageHeader header
    Body body
    constant text manifest = "manifest"
    /*
        This action gets the address of the documents cache.

        Attribute: Returns The document cache address. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.Attribute

        WebPage page
        text result = page:GetManifest()
        
    */
    action GetManifest returns text
        text textValue = ""
        Attribute value = me:GetAttribute(manifest)
        if value not= undefined
            textValue = value:GetValue()
        end
        return textValue
    end

    /*
        This action sets the address of the documents cache.

        Attribute: Parameter value The document cache address. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.Attribute

        WebPage page
        page:SetManifest("test.appcache")
        
    */
    action SetManifest(text value)
        Attribute attribute
        attribute:SetName(manifest)
        attribute:SetValue(value)
        me:Add(attribute)
    end

    /*
        This action gets the body of the webpage.

        Attribute: Returns The body of the webpage. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.Body

        WebPage page
        Body result = page:GetBody()
        
    */
    action GetBody returns Body
        return body
    end

    /*
        This action gets the type of document.

        Attribute: Returns The type of document. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.DocumentType

        WebPage page
        DocumentType result = page:GetDocumentType()
        
    */
    action GetDocumentType returns DocumentType
        return documentType
    end

    /*
        This action sets the DocumentType of the webpage.

        Attribute: Parameter doc The document type. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.DocumentType

        WebPage page
        DocumentType document
        page:SetDocumentType(document)
        
    */
    action SetDocumentType(DocumentType doc)
        documentType = doc
    end

    /*
        This action sets the title of the webpage.

        Attribute: Parameter value The title. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.Attribute

        WebPage page
        page:SetWebPageTitle("My First Webpage")
        
    */
    action SetWebPageTitle(text value)
        header:SetTitleHeaderValue(value)
    end

    action AddMetaData(MetaData meta)
        header:Add(meta)
    end
    /*
        This action gets the header of this webpage.

        Attribute: Returns The header. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.WebPageHeader

        WebPage page
        WebPageHeader result = page:GetWebPageHeader()
        
    */
    action GetWebPageHeader returns WebPageHeader
        return header
    end

    /*
        This action sets the address of the documents cache.

        Attribute: Parameter header The header. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.WebPageHeader

        WebPage page
        WebPageHeader header
        page:SetWebPageHeader(header)
        
    */
    action SetWebPageHeader(WebPageHeader header) 
        me:header = header
    end

    /*
        This action sets the body of the webpage.

        Attribute: Parameter body The body. 

        Attribute: Example
        use Libraries.Web.Page.WebPage
        use Libraries.Web.Page.Body

        WebPage page
        Body body
        page:SetBody(body)
        
    */
    action SetBody(Body body)
        me:body = body
    end

    action Generate returns text
        text result = documentType:Generate()
        result = result + "<html "
        Attributes attributes = me:GetAttributes()
        result = result + attributes:Generate()
        result = result + ">"
        
        if header not= undefined
            result = result + header:Generate()
        end

        result = result + body:Generate()
        return result + "</html>"
    end

    /*
        This action adds a link to a stylesheet in the header.

        Attribute: Parameter path The location of the stylesheet being added.
        
        Attribute: Returns The generated HeaderLink for any further manipulation

        Attribute: Example
        use Libraries.Web.Page.all

        WebPage page
        page:AddStylesheet("style.css")
    */
    action AddStylesheet(text path) returns HeaderLink
        return me:header:AddStylesheet(path)
    end
    
    /*
        This action adds a script in the header.

        Attribute: Parameter path The location of the script being added.
        
        Attribute: Returns The generated Script for any further manipulation

        Attribute: Example
        use Libraries.Web.Page.all

        WebPage page
        page:AddScript("script.js")
    */
    action AddScript(text path) returns Script
        Script script
        script:SetAddress(path)
        me:header:Add(script)
        return script
    end

    /*
        This action adds a link to an icon in the header. This is sometimes referred to as a "favicon".

        Attribute: Parameter path The location of the stylesheet being added. The file extension must be .ico
        
        Attribute: Returns The generated link for any further manipulation

        Attribute: Example
        use Libraries.Web.Page.all

        WebPage page
        page:AddIcon("favicon.ico")
    */
    action AddIcon(text path) returns HeaderLink
        return me:header:AddIcon(path)
    end

