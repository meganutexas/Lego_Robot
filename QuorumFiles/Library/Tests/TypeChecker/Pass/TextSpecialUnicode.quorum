class Main
    action Main
        text p = ""
        Text t 
        t:SetValue(p)
        
        integer i = 0
        text str = ""

        repeat 26 times
            str = str + p:GetUnicodeValue(65+i)
            i = i + 1
        end
        output str

        i = 0
        str = ""
        repeat 26 times
            str = str + t:GetUnicodeValue(65+i)
            i = i + 1
        end

        output str
    end
end