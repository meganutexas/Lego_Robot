use Libraries.Compute.Vector2
use Libraries.Compute.Math

class Vector2Angle 
    Math math
    action Main
        Vector2 vector
        vector:Set(0.5, math:SquareRoot(3.0) / 2.0)

        output vector:Angle()
        output vector:AngleInRadians()
        
        Vector2 vector1
        Vector2 vector2

        vector1:Set(4, 7)
        vector2:Set(3, 9)
        output vector1:Angle(vector2)
        output vector1:AngleInRadians(vector2)
    end
end